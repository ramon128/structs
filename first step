struct book {
        long ISBN-Nummer;
        string Buchtitel;
        string Buchautor;
        short Anzahl;
        char Liste;
};

//Variable der Struktur erstellen
struct book bookEtwas;                    //idk


// Zugriff auf die Elemente
bookEtwas.ISBN-Nummer = "";
strcpy(bookEtwas.Buchname, "");
strcpy(bookEtwas.Buchautor, "");
bookEtwas.Anzahl = "";
strcpy(bookEtwas.Liste, "");

printf("Buchname: %s\n", book.Buchname);
